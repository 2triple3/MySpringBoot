var star1 = function(value) {
  return parseInt(value)
}

var star2 = function(value) {
  if (value % 1 == 0) {
    return 0
  } else {
    return 1
  }
}
var star3 = function(value) {
  var b = parseInt(value)
  var a = 0
  if (value % 1 == 0) {
    a = 0
  } else {
    a = 1
  }
  return 5 - b - a
}

var filters = function(value, num) {
  return value.toFixed(num)
}


module.exports = {
  star1: star1,
  star2: star2,
  star3: star3,
  filters: filters
};